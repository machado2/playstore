{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^15.0.0",
    "react-scripts": "5.0.1",
    "relay-runtime": "^15.0.0"
  },
  "scripts": {
    "start": "npm run relay; concurrently --kill-others --names \"react-scripts,relay\" \"react-scripts start\" \"relay-compiler --watch\"",
    "build": "npm run relay && react-scripts build",
    "update-schema": "get-graphql-schema http://localhost:8080 > schema.graphql",
    "relay": "relay-compiler $@"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "babel-plugin-relay": "^15.0.0",
    "concurrently": "^8.2.1",
    "get-graphql-schema": "^2.1.2",
    "relay-compiler": "^15.0.0",
    "watchman": "^1.0.0"
  }
}
